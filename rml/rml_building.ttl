@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <ttp://xmlns.com/foaf/0.1/> .
@prefix wd: <https://www.wikidata.org/wiki/> .
@prefix ex: <http://example.com/> .
@prefix onto: <http://example.com/ontology/> .
@base <http://example.com/> .

<#Mapping> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "../data/data_set2/csv/building/Build.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://example.com/country/id/{b_id}" ;
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [
      rr:constant onto:Build
    ] ;
  ];

  rr:predicateObjectMap [
    rr:predicate rdf:label ;
    rr:objectMap [
      rml:reference "name"
    ] ;
  ];

  rr:predicateObjectMap [
    rr:predicate rdf:comment ;
    rr:objectMap [
      rml:reference "comment"
    ] ;
  ].
